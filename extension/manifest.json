{
  "name": "Browsing Bros",
  "version": "0.1",
  "description": "Allows users to save navigation history",
  "icons": {
    "100": "images/bb3.png"
  },
  "manifest_version": 2,

  "background": {
    "persistent": false,
    "scripts": ["jquery.js", "background.js"]
  },

  "permissions": [
    "tabs"
  ],

  "browser_action": {
    "default_icon": {
      "32": "images/bb3.png"
    },

    "default_title": "Browse Together",

    "default_popup": "popup.html"

  },

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
