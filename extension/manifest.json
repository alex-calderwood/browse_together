{
  "name": "Browsing Bros",
  "version": "0.1",
  "description": "Allows users to save navigation history",
  "icons": {
    "100": "images/bb3.png"
  },
  "manifest_version": 2,

  "background": {
    "persistent": false,
    "scripts": ["jquery.js", "background.js"]
  },

  "permissions": [
    "tabs"
  ],

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
